# Backend API URL (The Go backend address)
# Removing /api suffix as the backend routes are mounted at root level (e.g. /auth/x/login)
VITE_API_BASE_URL=http://localhost:8080

# X (Twitter) Authentication
# These are just placeholders in the example, actual keys move to backend
VITE_X_AUTH_CLIENT_ID=your_twitter_client_id_here
VITE_X_REDIRECT_URI=http://localhost:5173/auth/callback/twitter
